{
    "overrides": [
      {
        "files": ["*.js", "*.jsx", "*.mjs", "*.cjs"],
        "options": {
          "printWidth": 120,
          "tabWidth": 2,
          "useTabs": false,
          "semi": false,
          "singleQuote": true,
          "trailingComma": "none",
          "bracketSpacing": true,
          "jsxBracketSameLine": false,
          "arrowParens": "always",
          "rangeStart": 0,
          "parser": "babel",
          "requirePragma": false,
          "insertPragma": false,
          "proseWrap": "preserve"
        }
      }
    ]
  
  }